  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "comment.block",
        "punctuation.definition.comment",
        "string.quoted.docstring",
        "string.quoted.docstring punctuation.definition.string"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#333942"
      }
    },
    {
      "name": "Comment keywords",
      "scope": ["storage.type.class.jsdoc", "keyword.other.documentation", "punctuation.definition.block.tag"],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#333942"
      }
    },
    {
      "name": "Imports",
      "scope": [
        "storage.modifier.import",
        "storage.modifier.package",
        "storage.type.annotation",
        "punctuation.definition.annotation",
        "keyword.other.import",
        "keyword.other.package",
        "meta.import punctuation.terminator",
        "meta.package punctuation.terminator"
      ],
      "settings": {
        "foreground": "#384457"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder", "source", "meta.method.body.java"],
      "settings": {
        "foreground": "#df5e65"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
