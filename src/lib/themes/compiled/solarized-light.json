{
  "displayName": "Solarized Light",
  "name": "solarized-light",
  "type": "light",
  "semanticHighlighting": true,
  "colorReplacements": {
    "#073642": "st-ansiBlack",
    "#268bd2": "st-ansiBlue",
    "#002b36": "st-ansiBrightBlack",
    "#839496": "st-ansiBrightBlue",
    "#93a1a1": "st-ansiBrightCyan",
    "#586e75": "st-ansiBrightGreen",
    "#6c71c4": "st-ansiBrightMagenta",
    "#cb4b16": "st-ansiBrightRed",
    "#fdf6e3": "st-ansiBrightWhite",
    "#657b83": "st-ansiBrightYellow",
    "#2aa198": "st-ansiCyan",
    "#859900": "st-ansiGreen",
    "#d33682": "st-ansiMagenta",
    "#dc322f": "st-ansiRed",
    "#eee8d5": "st-ansiWhite",
    "#b58900": "st-ansiYellow"
  },
  "colors": {
    "editor.background": "st-bg",
    "editor.foreground": "st-fg",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiBrightBlack": "#002b36",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightGreen": "#586e75",
    "terminal.ansiBrightMagenta": "#6c71c4",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightWhite": "#fdf6e3",
    "terminal.ansiBrightYellow": "#657b83",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiYellow": "#b58900"
  },
  "tokenColors": [
    { "settings": { "foreground": "st-fg" } },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "variable.legacy.builtin.python"
      ],
      "settings": { "foreground": "st-174" }
    },
    { "scope": "comment", "settings": { "fontStyle": "italic", "foreground": "st-175" } },
    { "scope": "string", "settings": { "foreground": "st-176" } },
    { "scope": "string.regexp", "settings": { "foreground": "st-177" } },
    { "scope": "constant.numeric", "settings": { "foreground": "st-178" } },
    { "scope": ["variable.language", "variable.other"], "settings": { "foreground": "st-179" } },
    { "scope": "keyword", "settings": { "foreground": "st-180" } },
    { "scope": "storage", "settings": { "fontStyle": "bold", "foreground": "st-181" } },
    {
      "scope": ["entity.name.class", "entity.name.type", "entity.name.namespace", "entity.name.scope-resolution"],
      "settings": { "foreground": "st-182" }
    },
    { "scope": "entity.name.function", "settings": { "foreground": "st-179" } },
    { "scope": "punctuation.definition.variable", "settings": { "foreground": "st-180" } },
    {
      "scope": ["punctuation.section.embedded.begin", "punctuation.section.embedded.end"],
      "settings": { "foreground": "st-177" }
    },
    { "scope": ["constant.language", "meta.preprocessor"], "settings": { "foreground": "st-183" } },
    { "scope": ["support.function.construct", "keyword.other.new"], "settings": { "foreground": "st-182" } },
    { "scope": ["constant.character", "constant.other"], "settings": { "foreground": "st-182" } },
    { "scope": "entity.other.inherited-class", "settings": { "foreground": "st-184" } },
    { "scope": "variable.parameter", "settings": {} },
    { "scope": "entity.name.tag", "settings": { "foreground": "st-179" } },
    { "scope": "punctuation.definition.tag", "settings": { "foreground": "st-175" } },
    { "scope": "entity.other.attribute-name", "settings": { "foreground": "st-175" } },
    { "scope": "support.function", "settings": { "foreground": "st-179" } },
    { "scope": "punctuation.separator.continuation", "settings": { "foreground": "st-177" } },
    { "scope": ["support.constant", "support.variable"], "settings": {} },
    { "scope": ["support.type", "support.class"], "settings": { "foreground": "st-180" } },
    { "scope": "support.type.exception", "settings": { "foreground": "st-182" } },
    { "scope": "support.other.variable", "settings": {} },
    { "scope": "invalid", "settings": { "foreground": "st-177" } },
    { "scope": ["meta.diff", "meta.diff.header"], "settings": { "fontStyle": "italic", "foreground": "st-179" } },
    { "scope": "markup.deleted", "settings": { "foreground": "st-177" } },
    { "scope": "markup.changed", "settings": { "foreground": "st-182" } },
    { "scope": "markup.inserted", "settings": { "foreground": "st-180" } },
    { "scope": "markup.quote", "settings": { "foreground": "st-180" } },
    { "scope": "markup.list", "settings": { "foreground": "st-183" } },
    { "scope": ["markup.bold", "markup.italic"], "settings": { "foreground": "st-178" } },
    { "scope": "markup.bold", "settings": { "fontStyle": "bold" } },
    { "scope": "markup.italic", "settings": { "fontStyle": "italic" } },
    { "scope": "markup.strikethrough", "settings": { "fontStyle": "strikethrough" } },
    { "scope": "markup.inline.raw", "settings": { "foreground": "st-176" } },
    { "scope": "markup.heading", "settings": { "fontStyle": "bold", "foreground": "st-179" } },
    { "scope": "markup.heading.setext", "settings": { "foreground": "st-179" } }
  ]
}
