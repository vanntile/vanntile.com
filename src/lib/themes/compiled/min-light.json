{
  "displayName": "Min Light",
  "name": "min-light",
  "type": "light",
  "semanticHighlighting": false,
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#212121",
    "terminal.ansiBlack": "st-ansiBlack",
    "terminal.ansiBlue": "st-ansiBlue",
    "terminal.ansiBrightBlack": "st-ansiBrightBlack",
    "terminal.ansiBrightBlue": "st-ansiBrightBlue",
    "terminal.ansiBrightCyan": "st-ansiBrightCyan",
    "terminal.ansiBrightGreen": "st-ansiBrightGreen",
    "terminal.ansiBrightMagenta": "st-ansiBrightMagenta",
    "terminal.ansiBrightRed": "st-ansiBrightRed",
    "terminal.ansiBrightWhite": "st-ansiBrightWhite",
    "terminal.ansiBrightYellow": "st-ansiBrightYellow",
    "terminal.ansiCyan": "st-ansiCyan",
    "terminal.ansiGreen": "st-ansiGreen",
    "terminal.ansiMagenta": "st-ansiMagenta",
    "terminal.ansiRed": "st-ansiRed",
    "terminal.ansiWhite": "st-ansiWhite",
    "terminal.ansiYellow": "st-ansiYellow"
  },
  "tokenColors": [
    { "settings": { "foreground": "st-fg" } },
    {
      "scope": [
        "keyword.operator.accessor",
        "meta.group.braces.round.function.arguments",
        "meta.template.expression",
        "markup.fenced_code meta.embedded.block"
      ],
      "settings": { "foreground": "st-70" }
    },
    { "scope": "emphasis", "settings": { "fontStyle": "italic" } },
    { "scope": ["strong", "markup.heading.markdown", "markup.bold.markdown"], "settings": { "fontStyle": "bold" } },
    { "scope": ["markup.italic.markdown"], "settings": { "fontStyle": "italic" } },
    { "scope": "meta.link.inline.markdown", "settings": { "fontStyle": "underline", "foreground": "st-71" } },
    { "scope": ["string", "markup.fenced_code", "markup.inline"], "settings": { "foreground": "st-72" } },
    { "scope": ["comment", "string.quoted.docstring.multi"], "settings": { "foreground": "st-73" } },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.other.placeholder",
        "constant.character.format.placeholder",
        "variable.language.this",
        "variable.other.object",
        "variable.other.class",
        "variable.other.constant",
        "meta.property-name",
        "meta.property-value",
        "support"
      ],
      "settings": { "foreground": "st-71" }
    },
    {
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type",
        "storage.control.clojure",
        "entity.name.function.clojure",
        "entity.name.tag.yaml",
        "support.function.node",
        "support.type.property-name.json",
        "punctuation.separator.key-value",
        "punctuation.definition.template-expression"
      ],
      "settings": { "foreground": "st-74" }
    },
    { "scope": "variable.parameter.function", "settings": { "foreground": "st-75" } },
    {
      "scope": [
        "support.function",
        "entity.name.type",
        "entity.other.inherited-class",
        "meta.function-call",
        "meta.instance.constructor",
        "entity.other.attribute-name",
        "entity.name.function",
        "constant.keyword.clojure"
      ],
      "settings": { "foreground": "st-76" }
    },
    {
      "scope": [
        "entity.name.tag",
        "string.quoted",
        "string.regexp",
        "string.interpolated",
        "string.template",
        "string.unquoted.plain.out.yaml",
        "keyword.other.template"
      ],
      "settings": { "foreground": "st-77" }
    },
    { "scope": "token.info-token", "settings": { "foreground": "st-78" } },
    { "scope": "token.warn-token", "settings": { "foreground": "st-79" } },
    { "scope": "token.error-token", "settings": { "foreground": "st-80" } },
    { "scope": "token.debug-token", "settings": { "foreground": "st-81" } },
    { "scope": ["strong", "markup.heading.markdown", "markup.bold.markdown"], "settings": { "foreground": "st-76" } },
    {
      "scope": [
        "punctuation.definition.arguments",
        "punctuation.definition.dict",
        "punctuation.separator",
        "meta.function-call.arguments"
      ],
      "settings": { "foreground": "st-82" }
    },
    {
      "scope": ["markup.underline.link", "punctuation.definition.metadata.markdown"],
      "settings": { "foreground": "st-77" }
    },
    { "scope": ["beginning.punctuation.definition.list.markdown"], "settings": { "foreground": "st-76" } },
    {
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": { "foreground": "st-83" }
    }
  ]
}
