      "scope": "markup.deleted",
      "settings": {
        "foreground": "--red-fg-21"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "--red-fg-22"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "--red-fg-18"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "--red-fg-3"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "--red-fg-5"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "--red-fg-8"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "--red-fs-1"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "--red-fs-0"
      }
    },
    {
      "scope": "markup.strikethrough",
