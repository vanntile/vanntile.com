{
  "displayName": "moonlight-ii",
  "name": "moonlight-ii",
  "type": "dark",
  "semanticHighlighting": false,
  "colorReplacements": {
    "#c8d3f5": "st-ansiBrightWhite",
    "#000000": "st-ansiBlack",
    "#82aaff": "st-ansiBrightBlue",
    "#86e1fc": "st-ansiBrightCyan",
    "#c3e88d": "st-ansiBrightGreen",
    "#fca7ea": "st-ansiBrightMagenta",
    "#ff757f": "st-ansiBrightRed",
    "#ffc777": "st-ansiBrightYellow",
    "#828bb8": "st-ansiBrightBlack"
  },
  "colors": {
    "editor.background": "st-bg",
    "editor.foreground": "st-fg",
    "terminal.ansiWhite": "#c8d3f5",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#82aaff",
    "terminal.ansiCyan": "#86e1fc",
    "terminal.ansiGreen": "#c3e88d",
    "terminal.ansiMagenta": "#fca7ea",
    "terminal.ansiRed": "#ff757f",
    "terminal.ansiYellow": "#ffc777",
    "terminal.ansiBrightWhite": "#c8d3f5",
    "terminal.ansiBrightBlack": "#828bb8",
    "terminal.ansiBrightBlue": "#82aaff",
    "terminal.ansiBrightCyan": "#86e1fc",
    "terminal.ansiBrightGreen": "#c3e88d",
    "terminal.ansiBrightMagenta": "#fca7ea",
    "terminal.ansiBrightRed": "#ff757f",
    "terminal.ansiBrightYellow": "#ffc777"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "string.quoted.docstring"],
      "settings": { "foreground": "st-84" }
    },
    {
      "name": "Variables and Plain Text",
      "scope": ["variable", "support.variable", "string constant.other.placeholder", "text.html"],
      "settings": { "foreground": "st-85" }
    },
    {
      "name": "DOM Variables",
      "scope": [
        "support.variable.dom",
        "support.constant.math",
        "support.type.object.module",
        "support.variable.object.process",
        "support.constant.json"
      ],
      "settings": { "foreground": "st-86" }
    },
    {
      "name": "Nil",
      "scope": ["constant.language.undefined", "constant.language.null"],
      "settings": { "foreground": "st-87" }
    },
    { "name": "PHP Constants", "scope": ["constant.other.php"], "settings": { "foreground": "st-86" } },
    { "name": "Colors", "scope": ["constant.other.color"], "settings": { "foreground": "st-88" } },
    { "name": "Invalid", "scope": ["invalid", "invalid.illegal"], "settings": { "foreground": "st-89" } },
    { "name": "Invalid deprecated", "scope": ["invalid.deprecated"], "settings": { "foreground": "st-90" } },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier", "keyword.other.important"],
      "settings": { "foreground": "st-90" }
    },
    { "name": "Keyword, Storage", "scope": ["keyword.control", "storage"], "settings": {} },
    {
      "name": "Interpolation",
      "scope": ["punctuation.definition.template-expression", "punctuation.section.embedded"],
      "settings": { "foreground": "st-91" }
    },
    {
      "name": "Spread",
      "scope": ["keyword.operator.spread", "keyword.operator.rest"],
      "settings": { "foreground": "st-92", "fontStyle": "bold" }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.operator",
        "keyword.control",
        "punctuation",
        "punctuation.definition.string",
        "punctuation.support.type.property-name",
        "text.html.vue-html meta.tag",
        "punctuation.definition.keyword",
        "punctuation.terminator.rule",
        "punctuation.definition.entity",
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.block.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "meta.property-list",
        "meta.brace.square",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": { "foreground": "st-91" }
    },
    { "name": "Keyword Control", "scope": ["keyword.control"], "settings": {} },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag", "markup.deleted.git_gutter"],
      "settings": { "foreground": "st-92" }
    },
    {
      "name": "Function, Special Method",
      "scope": ["entity.name.function", "variable.function", "keyword.other.special-method"],
      "settings": { "foreground": "st-93" }
    },
    {
      "name": "Support Function",
      "scope": ["support.function", "meta.function-call entity.name.function"],
      "settings": { "foreground": "st-94" }
    },
    {
      "name": "C-related Block Level Variables",
      "scope": ["source.cpp meta.block variable.other"],
      "settings": { "foreground": "st-92" }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": { "foreground": "st-92" }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "variable.other.constant",
        "constant.language",
        "keyword.other.type.php",
        "storage.type.php",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit"
      ],
      "settings": { "foreground": "st-95" }
    },
    {
      "name": "Number, Boolean",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.json",
        "constant.language.infinity",
        "constant.language.nan"
      ],
      "settings": { "foreground": "st-96" }
    },
    {
      "name": "Function Argument",
      "scope": [
        "variable.parameter.function.language.special",
        "variable.parameter",
        "meta.function.parameter variable"
      ],
      "settings": { "foreground": "st-97" }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "meta.attribute-selector"
      ],
      "settings": { "foreground": "st-98" }
    },
    { "name": "Object", "scope": ["variable.other.object"], "settings": { "foreground": "st-86" } },
    {
      "name": "Object Key",
      "scope": [
        "meta.object-literal.key",
        "string.alias.graphql",
        "string.unquoted.graphql",
        "string.unquoted.alias.graphql",
        "meta.field.declaration.ts variable.object.property",
        "variable.object.property"
      ],
      "settings": { "foreground": "st-99" }
    },
    {
      "name": "Nested Object Property",
      "scope": ["meta.object.member", "variable.other.object.property"],
      "settings": { "foreground": "st-100" }
    },
    {
      "name": "Object Property",
      "scope": ["variable.other.property", "support.variable.property", "support.variable.property.dom"],
      "settings": { "foreground": "st-100" }
    },
    {
      "name": "Haskell Constants",
      "scope": ["source.haskell constant.other.haskell"],
      "settings": { "foreground": "st-95" }
    },
    {
      "name": "Haskell Imports",
      "scope": ["source.haskell meta.import.haskell entity.name.namespace"],
      "settings": { "foreground": "st-85" }
    },
    {
      "name": "Types Fixes",
      "scope": ["source.haskell storage.type", "source.c storage.type", "source.java storage.type"],
      "settings": { "foreground": "st-86" }
    },
    { "name": "Lambda Arrow", "scope": ["storage.type.function"], "settings": { "foreground": "st-90" } },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": { "foreground": "st-86" }
    },
    { "name": "Entity types", "scope": ["support.type"], "settings": { "foreground": "st-96" } },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name",
        "support.type.property-name.css",
        "support.type.vendored.property-name"
      ],
      "settings": { "foreground": "st-93" }
    },
    {
      "name": "Sub-methods",
      "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
      "settings": { "foreground": "st-92" }
    },
    { "name": "Language methods", "scope": ["variable.language"], "settings": { "foreground": "st-92" } },
    { "name": "entity.name.method.js", "scope": ["entity.name.method.js"], "settings": { "foreground": "st-93" } },
    {
      "name": "meta.method.js",
      "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
      "settings": { "foreground": "st-93" }
    },
    { "name": "Attributes", "scope": ["entity.other.attribute-name"], "settings": { "foreground": "st-90" } },
    {
      "name": "HTML Attributes",
      "scope": ["text.html.basic entity.other.attribute-name.html", "text.html.basic entity.other.attribute-name"],
      "settings": { "foreground": "st-86" }
    },
    {
      "name": "HTML Doctype",
      "scope": ["meta.tag.metadata.doctype entity.name.tag", "meta.tag.metadata.doctype entity.other.attribute-name"],
      "settings": { "foreground": "st-90" }
    },
    { "name": "CSS Classes", "scope": ["entity.other.attribute-name.class"], "settings": { "foreground": "st-86" } },
    { "name": "CSS ID's", "scope": ["source.sass keyword.control"], "settings": { "foreground": "st-93" } },
    {
      "name": "CSS psuedo selectors",
      "scope": ["entity.other.attribute-name.pseudo-class", "entity.other.attribute-name.pseudo-element"],
      "settings": { "foreground": "st-99" }
    },
    {
      "name": "CSS Property value",
      "scope": ["support.constant.property-value"],
      "settings": { "foreground": "st-97" }
    },
    { "name": "Inserted", "scope": ["markup.inserted"], "settings": { "foreground": "st-98" } },
    { "name": "Deleted", "scope": ["markup.deleted"], "settings": { "foreground": "st-92" } },
    { "name": "Changed", "scope": ["markup.changed"], "settings": { "foreground": "st-90" } },
    { "name": "Regular Expressions", "scope": ["string.regexp"], "settings": { "foreground": "st-101" } },
    {
      "name": "Regular Expressions - Punctuation",
      "scope": ["punctuation.definition.group"],
      "settings": { "foreground": "st-92" }
    },
    {
      "name": "Regular Expressions - Character Class",
      "scope": ["constant.other.character-class.regexp", "keyword.control.anchor.regexp"],
      "settings": { "foreground": "st-90" }
    },
    {
      "name": "Regular Expressions - Character Class Set",
      "scope": ["constant.other.character-class.set.regexp"],
      "settings": { "foreground": "st-86" }
    },
    {
      "name": "Regular Expressions - Quantifier",
      "scope": ["keyword.operator.quantifier.regexp"],
      "settings": { "foreground": "st-97" }
    },
    { "name": "Escape Characters", "scope": ["constant.character.escape"], "settings": { "foreground": "st-91" } },
    { "name": "URL", "scope": ["*url*", "*link*", "*uri*"], "settings": { "fontStyle": "underline" } },
    {
      "name": "Decorators",
      "scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
      "settings": { "foreground": "st-93" }
    },
    { "name": "CSS Units", "scope": ["keyword.other.unit"], "settings": { "foreground": "st-102" } },
    {
      "name": "ES7 Bind Operator",
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": { "foreground": "st-92" }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": { "foreground": "st-93" }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "st-94" }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "st-92" }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "st-97" }
    },
    {
      "name": "JSON Key - Level 4",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "st-86" }
    },
    {
      "name": "JSON Key - Level 5",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "st-99" }
    },
    {
      "name": "JSON Key - Level 6",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": { "foreground": "st-93" }
    },
    {
      "name": "Plain Punctuation",
      "scope": ["punctuation.definition.list_item.markdown"],
      "settings": { "foreground": "st-103" }
    },
    {
      "name": "Block Punctuation",
      "scope": [
        "meta.block",
        "meta.brace",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.section.function"
      ],
      "settings": { "foreground": "st-104" }
    },
    {
      "name": "Markdown - Plain",
      "scope": ["meta.jsx.children", "meta.embedded.block"],
      "settings": { "foreground": "st-104" }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": { "foreground": "st-90" }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": ["text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"],
      "settings": { "foreground": "st-91" }
    },
    {
      "name": "Markdown - Heading punctuation",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": { "foreground": "st-91" }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic", "foreground": "st-92" }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": { "fontStyle": "bold", "foreground": "st-92" }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": { "fontStyle": "bold", "foreground": "st-92" }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": { "fontStyle": "underline", "foreground": "st-96" }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": { "foreground": "st-91" }
    },
    { "name": "Markup - Quote", "scope": ["markup.quote"], "settings": { "fontStyle": "italic" } },
    { "name": "Markdown - Link", "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "st-93" } },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": { "foreground": "st-90" }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": { "foreground": "st-86" }
    },
    { "name": "Markup - Raw Block", "scope": ["markup.raw.block"], "settings": { "foreground": "st-90" } },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": ["markup.fenced_code.block.markdown", "markup.inline.raw.string.markdown"],
      "settings": { "foreground": "st-91" }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": { "foreground": "st-91" }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": { "fontStyle": "bold", "foreground": "st-91" }
    },
    { "name": "Markup - Table", "scope": ["markup.table"], "settings": { "foreground": "st-103" } },
    { "scope": "token.info-token", "settings": { "foreground": "st-94" } },
    { "scope": "token.warn-token", "settings": { "foreground": "st-86" } },
    { "scope": "token.error-token", "settings": { "foreground": "st-92" } },
    { "scope": "token.debug-token", "settings": { "foreground": "st-90" } }
  ]
}
