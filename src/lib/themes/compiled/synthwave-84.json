{
  "displayName": "Synthwave '84",
  "name": "synthwave-84",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#262335",
    "terminal.ansiBlue": "st-ansiBlue",
    "terminal.ansiBrightBlue": "st-ansiBrightBlue",
    "terminal.ansiBrightCyan": "st-ansiBrightCyan",
    "terminal.ansiBrightGreen": "st-ansiBrightGreen",
    "terminal.ansiBrightMagenta": "st-ansiBrightMagenta",
    "terminal.ansiBrightRed": "st-ansiBrightRed",
    "terminal.ansiBrightYellow": "st-ansiBrightYellow",
    "terminal.ansiCyan": "st-ansiCyan",
    "terminal.ansiGreen": "st-ansiGreen",
    "terminal.ansiMagenta": "st-ansiMagenta",
    "terminal.ansiRed": "st-ansiRed",
    "terminal.ansiYellow": "st-ansiYellow"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "string.quoted.docstring.multi.python",
        "string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
        "string.quoted.docstring.multi.python punctuation.definition.string.end.python"
      ],
      "settings": { "fontStyle": "italic", "foreground": "st-185" }
    },
    {
      "scope": ["string.quoted", "string.template", "punctuation.definition.string"],
      "settings": { "foreground": "st-186" }
    },
    { "scope": "string.template meta.embedded.line", "settings": { "foreground": "st-187" } },
    { "scope": ["variable", "entity.name.variable"], "settings": { "foreground": "st-188" } },
    { "scope": "variable.language", "settings": { "fontStyle": "bold", "foreground": "st-189" } },
    { "scope": "variable.parameter", "settings": { "fontStyle": "italic" } },
    { "scope": ["storage.type", "storage.modifier"], "settings": { "foreground": "st-190" } },
    { "scope": "constant", "settings": { "foreground": "st-191" } },
    { "scope": "string.regexp", "settings": { "foreground": "st-191" } },
    { "scope": "constant.numeric", "settings": { "foreground": "st-191" } },
    { "scope": "constant.language", "settings": { "foreground": "st-191" } },
    { "scope": "constant.character.escape", "settings": { "foreground": "st-192" } },
    { "scope": "entity.name", "settings": { "foreground": "st-189" } },
    { "scope": "entity.name.tag", "settings": { "foreground": "st-193" } },
    { "scope": ["punctuation.definition.tag"], "settings": { "foreground": "st-192" } },
    { "scope": "entity.other.attribute-name", "settings": { "foreground": "st-190" } },
    { "scope": "entity.other.attribute-name.html", "settings": { "fontStyle": "italic", "foreground": "st-190" } },
    { "scope": ["entity.name.type", "meta.attribute.class.html"], "settings": { "foreground": "st-189" } },
    { "scope": "entity.other.inherited-class", "settings": { "foreground": "st-194" } },
    { "scope": ["entity.name.function", "variable.function"], "settings": { "foreground": "st-192" } },
    { "scope": ["keyword.control.export.js", "keyword.control.import.js"], "settings": { "foreground": "st-193" } },
    { "scope": ["constant.numeric.decimal.js"], "settings": { "foreground": "st-195" } },
    { "scope": "keyword", "settings": { "foreground": "st-190" } },
    { "scope": "keyword.control", "settings": { "foreground": "st-190" } },
    { "scope": "keyword.operator", "settings": { "foreground": "st-190" } },
    {
      "scope": ["keyword.operator.new", "keyword.operator.expression", "keyword.operator.logical"],
      "settings": { "foreground": "st-190" }
    },
    { "scope": "keyword.other.unit", "settings": { "foreground": "st-191" } },
    { "scope": "support", "settings": { "foreground": "st-189" } },
    { "scope": "support.function", "settings": { "foreground": "st-192" } },
    { "scope": "support.variable", "settings": { "foreground": "st-188" } },
    { "scope": ["meta.object-literal.key", "support.type.property-name"], "settings": { "foreground": "st-188" } },
    { "scope": "punctuation.separator.key-value", "settings": { "foreground": "st-187" } },
    { "scope": "punctuation.section.embedded", "settings": { "foreground": "st-190" } },
    {
      "scope": ["punctuation.definition.template-expression.begin", "punctuation.definition.template-expression.end"],
      "settings": { "foreground": "st-193" }
    },
    {
      "scope": ["support.type.property-name.css", "support.type.property-name.json"],
      "settings": { "foreground": "st-193" }
    },
    { "scope": "switch-block.expr.js", "settings": { "foreground": "st-193" } },
    {
      "scope": "variable.other.constant.property.js, variable.other.property.js",
      "settings": { "foreground": "st-196" }
    },
    { "scope": "constant.other.color", "settings": { "foreground": "st-191" } },
    { "scope": "support.constant.font-name", "settings": { "foreground": "st-191" } },
    { "scope": "entity.other.attribute-name.id", "settings": { "foreground": "st-192" } },
    {
      "scope": ["entity.other.attribute-name.pseudo-element", "entity.other.attribute-name.pseudo-class"],
      "settings": { "foreground": "st-194" }
    },
    { "scope": "support.function.misc.css", "settings": { "foreground": "st-189" } },
    { "scope": ["markup.heading", "entity.name.section"], "settings": { "foreground": "st-188" } },
    { "scope": ["text.html", "keyword.operator.assignment"], "settings": { "foreground": "st-197" } },
    { "scope": "markup.quote", "settings": { "fontStyle": "italic", "foreground": "st-198" } },
    { "scope": "beginning.punctuation.definition.list", "settings": { "foreground": "st-188" } },
    { "scope": "markup.underline.link", "settings": { "foreground": "st-194" } },
    { "scope": "string.other.link.description", "settings": { "foreground": "st-191" } },
    { "scope": "meta.function-call.generic.python", "settings": { "foreground": "st-192" } },
    { "scope": "variable.parameter.function-call.python", "settings": { "foreground": "st-193" } },
    { "scope": "storage.type.cs", "settings": { "foreground": "st-189" } },
    { "scope": "entity.name.variable.local.cs", "settings": { "foreground": "st-188" } },
    {
      "scope": ["entity.name.variable.field.cs", "entity.name.variable.property.cs"],
      "settings": { "foreground": "st-188" }
    },
    { "scope": "constant.other.placeholder.c", "settings": { "fontStyle": "italic", "foreground": "st-193" } },
    {
      "scope": ["keyword.control.directive.include.c", "keyword.control.directive.define.c"],
      "settings": { "foreground": "st-193" }
    },
    { "scope": "storage.modifier.c", "settings": { "foreground": "st-189" } },
    { "scope": "source.cpp keyword.operator", "settings": { "foreground": "st-190" } },
    { "scope": "constant.other.placeholder.cpp", "settings": { "fontStyle": "italic", "foreground": "st-193" } },
    {
      "scope": ["keyword.control.directive.include.cpp", "keyword.control.directive.define.cpp"],
      "settings": { "foreground": "st-193" }
    },
    { "scope": "storage.modifier.specifier.const.cpp", "settings": { "foreground": "st-189" } },
    {
      "scope": ["source.elixir support.type.elixir", "source.elixir meta.module.elixir entity.name.class.elixir"],
      "settings": { "foreground": "st-192" }
    },
    { "scope": "source.elixir entity.name.function", "settings": { "foreground": "st-193" } },
    {
      "scope": ["source.elixir constant.other.symbol.elixir", "source.elixir constant.other.keywords.elixir"],
      "settings": { "foreground": "st-192" }
    },
    { "scope": "source.elixir punctuation.definition.string", "settings": { "foreground": "st-193" } },
    {
      "scope": [
        "source.elixir variable.other.readwrite.module.elixir",
        "source.elixir variable.other.readwrite.module.elixir punctuation.definition.variable.elixir"
      ],
      "settings": { "foreground": "st-193" }
    },
    {
      "scope": "source.elixir .punctuation.binary.elixir",
      "settings": { "fontStyle": "italic", "foreground": "st-188" }
    },
    { "scope": ["entity.global.clojure"], "settings": { "fontStyle": "bold", "foreground": "st-192" } },
    { "scope": ["storage.control.clojure"], "settings": { "fontStyle": "italic", "foreground": "st-192" } },
    {
      "scope": ["meta.metadata.simple.clojure", "meta.metadata.map.clojure"],
      "settings": { "fontStyle": "italic", "foreground": "st-189" }
    },
    { "scope": ["meta.quoted-expression.clojure"], "settings": { "fontStyle": "italic" } },
    { "scope": ["meta.symbol.clojure"], "settings": { "foreground": "st-199" } },
    { "scope": "source.go", "settings": { "foreground": "st-199" } },
    { "scope": "source.go meta.function-call.go", "settings": { "foreground": "st-192" } },
    {
      "scope": [
        "source.go keyword.package.go",
        "source.go keyword.import.go",
        "source.go keyword.function.go",
        "source.go keyword.type.go",
        "source.go keyword.const.go",
        "source.go keyword.var.go",
        "source.go keyword.map.go",
        "source.go keyword.channel.go",
        "source.go keyword.control.go"
      ],
      "settings": { "foreground": "st-190" }
    },
    {
      "scope": ["source.go storage.type", "source.go keyword.struct.go", "source.go keyword.interface.go"],
      "settings": { "foreground": "st-193" }
    },
    {
      "scope": ["source.go constant.language.go", "source.go constant.other.placeholder.go", "source.go variable"],
      "settings": { "foreground": "st-195" }
    },
    {
      "scope": ["markup.underline.link.markdown", "markup.inline.raw.string.markdown"],
      "settings": { "fontStyle": "italic", "foreground": "st-193" }
    },
    { "scope": ["string.other.link.title.markdown"], "settings": { "foreground": "st-190" } },
    {
      "scope": ["markup.heading.markdown", "entity.name.section.markdown"],
      "settings": { "fontStyle": "bold", "foreground": "st-188" }
    },
    { "scope": ["markup.italic.markdown"], "settings": { "fontStyle": "italic", "foreground": "st-195" } },
    { "scope": ["markup.bold.markdown"], "settings": { "fontStyle": "bold", "foreground": "st-195" } },
    {
      "scope": ["punctuation.definition.quote.begin.markdown", "markup.quote.markdown"],
      "settings": { "foreground": "st-193" }
    },
    { "scope": ["source.dart", "source.python", "source.scala"], "settings": { "foreground": "st-199" } },
    { "scope": ["string.interpolated.single.dart"], "settings": { "foreground": "st-191" } },
    { "scope": ["variable.parameter.dart"], "settings": { "foreground": "st-193" } },
    { "scope": ["constant.numeric.dart"], "settings": { "foreground": "st-195" } },
    { "scope": ["variable.parameter.scala"], "settings": { "foreground": "st-195" } },
    { "scope": ["meta.template.expression.scala"], "settings": { "foreground": "st-193" } }
  ]
}
